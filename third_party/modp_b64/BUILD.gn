config("modp_b64_warnings") {
  if (is_clang) {
    cflags_cc = [ "-Wno-unreachable-code-break" ]
  }
}

static_library("modp_b64") {
  sources = [
    "modp_b64.cc",
    "modp_b64.h",
    "modp_b64_data.h",
  ]
  configs += [ ":modp_b64_warnings" ]
}
